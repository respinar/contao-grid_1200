				"assets/contao/css/grid-1200-rtl-uncompressed.css": "assets/contao/css/grid-1200-rtl-uncompressed.css",
				"assets/contao/css/grid-1200-rtl.css": "assets/contao/css/grid-1200-rtl.css",
				"assets/contao/css/grid-1200-rtl-wrapper-uncompressed.css": "assets/contao/css/grid-1200-rtl-wrapper-uncompressed.css",
				"assets/contao/css/grid-1200-rtl-wrapper.css": "assets/contao/css/grid-1200-rtl-wrapper.css"
			},
			"transifex": {
				"project": "contao-grid_1200",
				"prefix": "master-",
				"languages_cto": "system/modules/grid_1200/languages",
				"languages_tx": ".tx"
			},
			"runonce":[

			]
		}
	}
