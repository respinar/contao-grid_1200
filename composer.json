	"keywords":["contao","grid","responsive","1200px","rtl"],
	"type":"contao-module",
	"homepage":"http://respinar.org",
	"license":"GPL-3.0",
	"authors":[
		{
			"name":"Hamid Abbaszadeh",
			"homepage":"http://contao.ninja",
			"role":"Developer"
		}
	],
	"support":{
		"issues":"https://github.com/respinar/contao_grid_1200/issues",
		"source":"https://github.com/respinar/contao_grid_1200"
	},
	"require":{
		"php":">=5.3",
		"contao/core":">=3.2,<4",
		"contao-community-alliance/composer-plugin": "*"
	},
	"autoload":{
		"classmap":[""]
	},
	"extra":{
		"contao":{
			"sources":{
				"system/modules/grid_16_columns": "system/modules/grid_16_columns",
